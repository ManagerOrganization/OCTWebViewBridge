{
  "name": "OCTWebViewBridge",
  "version": "1.0.4",
  "summary": "WKWebView Javascript Bridge",
  "description": "WKWebView Bridge, Javascript call Native Code.",
  "homepage": "https://github.com/octree/OCTWebViewBridge",
  "license": "MIT",
  "authors": {
    "Octree": "fouljz@gmail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/octree/OCTWebViewBridge.git",
    "tag": "1.0.4"
  },
  "source_files": [
    "Classes",
    "Classes/**/*.{h,m}"
  ],
  "public_header_files": "Classes/**/*.h",
  "resource_bundles": {
    "OCTWebViewBridge": [
      "Resources/*.{js}"
    ]
  },
  "ios": {
    "preserve_paths": [
      "Classes",
      "Classes/**/*.{h,m}",
      "Resources/*.{js}",
      "OCTWebViewBridge.framework"
    ]
  },
  "frameworks": "WebKit"
}
